{
  "name": "uxondynamics",
  "version": "0.0.1",
  "description": "UDX â€” Star Citizen companion app by Uxon Dynamics",
  "main": "./out/main/index.js",
  "author": "https://uxondynamics.com",
  "homepage": "https://uxondynamics.com",
  "scripts": {
    "sync-envx": "tsx ./dev/mts/sync-envx.mts",
    "create:component": "tsx ./dev/mts/create-udx-component.mts",
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "npm run sync-envx && dotenvx run -f ./env/.env -- electron-vite preview",
    "dev": "npm run sync-envx && dotenvx run -f ./env/.env -- electron-vite dev",
    "build": "npm run sync-envx && dotenvx run -f ./env/.env -- npm run typecheck && electron-vite build",
    "build:unpack": "npm run sync-envx && dotenvx run -f ./env/.env -- npm run build && electron-builder --dir",
    "build:win": "npm run sync-envx && dotenvx run -f ./env/.env -- npm run build && electron-builder --win",
    "build:mac": "npm run sync-envx && dotenvx run -f ./env/.env -- npm run build && electron-builder --mac",
    "build:linux": "npm run sync-envx && dotenvx run -f ./env/.env -- npm run build && electron-builder --linux",
    "postinstall": "npm run sync-envx && dotenvx run -f ./env/.env -- electron-builder install-app-deps"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.51.0",
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@react-three/drei": "^10.7.6",
    "@react-three/fiber": "^9.3.0",
    "@supabase/supabase-js": "^2.58.0",
    "@tailwindcss/vite": "^4.1.14",
    "@tanstack/react-query": "^5.90.2",
    "@types/three": "^0.180.0",
    "axios": "^1.12.2",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "cross-env": "^10.1.0",
    "discord-rpc": "^4.0.1",
    "draco3d": "^1.5.7",
    "electron-log": "^5.4.3",
    "electron-store": "^11.0.0",
    "electron-updater": "^6.3.9",
    "fs-extra": "^11.3.2",
    "gsap": "^3.13.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-electron-fs-backend": "^3.0.3",
    "i18next-http-backend": "^3.0.2",
    "lucide-react": "^0.544.0",
    "posthog-js": "^1.270.1",
    "react-i18next": "^16.0.0",
    "react-router": "^7.9.3",
    "react-router-dom": "^7.9.3",
    "sharp": "^0.34.4",
    "socket.io-client": "^4.8.1",
    "tailwindcss": "^4.1.14",
    "three": "^0.180.0",
    "vite-plugin-static-copy": "^3.1.3",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.1.0",
    "@electron-toolkit/tsconfig": "^2.0.0",
    "@types/discord-rpc": "^4.0.9",
    "@types/node": "^22.18.6",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "electron": "^38.1.2",
    "electron-builder": "^25.1.8",
    "electron-vite": "^4.0.1",
    "eslint": "^9.36.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "prettier": "^3.6.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2",
    "vite": "^7.1.6"
  }
}
